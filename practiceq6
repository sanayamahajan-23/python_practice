def fibonacci(n):
    fib_sequence = [0, 1]
    for i in range(2, n):
        next_term = fib_sequence[-1] + fib_sequence[-2]
        fib_sequence.append(next_term)
    return fib_sequence
n = int(input("Enter the number of terms for Fibonacci series: "))
fib_series = fibonacci(n)
print("Fibonacci series up to", n, "terms:", fib_series)
