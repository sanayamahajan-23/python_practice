def largestamongthree(x, y, z):
    if x > y and x > z:
        print(x,"is the largest of three.")
    elif y > x and y > z:
        print(y,"is the greatest of three.")
    elif z > x and z > y:
        print(z,"is the greatest of three.")
    else:
        print("All are equal")

num1 = int(input("Enter the first number: "))
num2 = int(input("Enter the second number: "))
num3 = int(input("Enter the third number: "))

largestamongthree(num1, num2, num3)
